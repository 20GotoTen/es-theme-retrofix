<theme>
	<formatVersion>7</formatVersion>
	<include>./../theme.xml</include>
<include>./../_inc/system/hidenames.xml</include>

	<view name="system">
		<image name="startSet" extra="static"></image>


<video name="backgroundvideo" extra="true">
			<size>0.770833333333333 1</size>
		</video>


	</view>

	<view name="detailed">
		<textlist name="gamelist">
			<selectorColor>5DB8D0</selectorColor>
			<selectedColor>ffffff</selectedColor>
		</textlist>
	</view>

	<view name="grid">
		<image name="gridtile.favorite">
			<color>5DB8D0ff</color>
		</image>  
	</view>

	<customView name="Tiles" inherits="grid">
		<gridtile name="selected">
			<backgroundEdgeColor>5DB8D0</backgroundEdgeColor>
		</gridtile>
	</customView>

</theme>
