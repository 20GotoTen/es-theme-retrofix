<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="background" extra="true">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<maxSize>1 1</maxSize>
			<path>./../_inc/assets/black/background.png</path>
			<path>./_inc/assets/black/background.png</path>
			<zIndex>0</zIndex>
		</image>

		<carousel name="systemcarousel">
			<minLogoOpacity>0.15</minLogoOpacity>
		</carousel>



	<image name="menuBar" extra="static">
			<path>./../_inc/assets/black/menuBar.png</path>
			<path>./_inc/assets/black/menuBar.png</path>
		<size>0.2604166666666667 1</size>
		<pos>0 0</pos>
		<origin>0 0</origin>
		<zIndex>20</zIndex>
		<linearSmooth>true</linearSmooth>
	</image>






	</view>

	<view name="system, detailed, grid">
		<image name="background" extra="true">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<path>./_inc/assets/black/background.png</path>
			<path>./../_inc/assets/black/background.png</path>
			<zIndex>0</zIndex>
		</image>
	</view>

	<view name="menu">
		<menuBackground name="menubg">
			<color>2F2F2F</color>
			<cornerSize>32 32</cornerSize>
		</menuBackground>
	</view>

	<view name="detailed">
		<image name="background2" extra="true">
			<path>./_inc/assets/black/detailed.png</path>
			<path>./../_inc/assets/black/detailed.png</path>
			<size>1 1</size>
			<pos>0 0</pos>
			<origin>0 0</origin>
			<zIndex>11</zIndex>
		</image>
	</view>

	<view name="system">
		<image name="background2" extra="static">
			<path>./../_inc/assets/black/overlay.png</path>
			<path>./_inc/assets/black/overlay.png</path>
			<size>1 1</size>
			<pos>0 0</pos>
			<origin>0 0</origin>
			<zIndex>30</zIndex>
		</image>
	</view>

	<view name="grid">
		<image name="gridOverlay" extra="true">
			<path>./../_inc/assets/black/gridoverlay.png</path>
			<path>./_inc/assets/black/gridoverlay.png</path>
			<size>1 1</size>
			<pos>0 0</pos>
			<origin>0 0</origin>
			<zIndex>8</zIndex>
		</image>
	</view>


	<customView name="Boxes" inherits="grid">
		<image name="gridOverlay" extra="true">
			<path>./../_inc/assets/black/tileoverlay.png</path>
			<path>./_inc/assets/black/tileoverlay.png</path>
			<size>1 1</size>
			<pos>0 0</pos>
			<origin>0 0</origin>
			<zIndex>8</zIndex>
		</image>
	</customView>





	<customView name="Boxes+Info" inherits="grid">
			<image name="gridOverlay" extra="true">
			<path>./../_inc/assets/black/boxDetailOverlay.png</path>
			<path>./_inc/assets/black/boxDetailOverlay.png</path>
		</image>
	</customView>




</theme>