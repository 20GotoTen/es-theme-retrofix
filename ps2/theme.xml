<theme>
	<formatVersion>7</formatVersion>
	<include>./../theme.xml</include>
<include>./../_inc/system/hidenames.xml</include>

  <view name="system">
   <image name="startSet" extra="static"></image>
 </view>

	<view name="detailed">
		<textlist name="gamelist">
			<selectorColor>5354BB</selectorColor>
						<selectorColorEnd>1EE0EF</selectorColorEnd>
			<selectorGradientType>default</selectorGradientType>
		</textlist>
	</view>

<view name="grid">
  <image name="gridtile.favorite">
    <color>5354BBff</color>
  </image>  
</view>

<customView name="Tiles" inherits="grid">
  <gridtile name="selected">
    <backgroundEdgeColor>5354BB</backgroundEdgeColor>
  </gridtile>
</customView>

</theme>
